booktitle = {Information Fusion (FUSION), 2013 16th International Conference on},
file = {:home/gustavo/Documents/Mendeley/Meissner et al/Meissner et al. - 2013 - Road user tracking using a Dempster-Shafer based classifying multiple-model PHD filter.pdf:pdf},
